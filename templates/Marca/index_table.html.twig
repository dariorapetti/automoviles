{
  "data": [
    {% for entity in entities %}
    [
      "{{ entity.id }}",
      "{{ entity.nombre|default('-') }}",
      {
        "show" : " {{ path( "marca_show", { "id": entity.id }) }}"
        ,"edit" : " {{ path( "marca_edit", { "id": entity.id }) }}"
        ,"delete" : " {{ path( "marca_delete", { "id": entity.id }) }}"
      }
    ]{{ (loop.last ? '' : ',') }}
    {% endfor %}
  ],
  "draw": {{ draw }},
  "recordsTotal": {{ totalRows }},
  "recordsFiltered": {{ totalFiltered }}
}