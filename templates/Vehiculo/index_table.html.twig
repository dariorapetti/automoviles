{
  "data": [
  {% for entity in entities %}
    [
      "{{ entity.id }}",
      "{{ entity.tipo_vehiculo }}",
      "{{ entity.cliente_proveedor }}",
      "{{ entity.marca }}",
      "{{ entity.modelo }}",
      "{{ entity.dominio }}",
      "{{ entity.cotizacion_dolar|money_format }}",
      "{{ entity.precio_compra_usd|money_format('U$S') }}",
      "{{ entity.precio_venta_usd|money_format('U$S') }}",
      "{{ entity.precio_compra_pesos|money_format }}",
      "{{ entity.precio_venta_pesos|money_format }}",
      {
        "show" : " {{ path( "vehiculo_show", { "id": entity.id }) }}"
        ,"edit" : " {{ path( "vehiculo_edit", { "id": entity.id }) }}"
        ,"delete" : " {{ path( "vehiculo_delete", { "id": entity.id }) }}"
      }
    ]{{ (loop.last ? '' : ',') }}
{% endfor %}
  ],
  "draw": {{ draw }},
  "recordsTotal": {{ totalRows }},
  "recordsFiltered": {{ totalFiltered }}
}