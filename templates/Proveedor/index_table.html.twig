{
  "data": [
    {% for entity in entities %}
    [
      "{{ entity.id }}",
      "{{ entity.razonSocial|default('-') }}",
      "{{ entity.direccion|default('-') }}",
      "{{ entity.telefonoOperador|default('-') }}",
      {
        "show" : " {{ path( "proveedor_show", { "id": entity.id }) }}"
        ,"edit" : " {{ path( "proveedor_edit", { "id": entity.id }) }}"
        ,"delete" : " {{ path( "proveedor_delete", { "id": entity.id }) }}"
      }
    ]{{ (loop.last ? '' : ',') }}
    {% endfor %}
  ],
  "draw": {{ draw }},
  "recordsTotal": {{ totalRows }},
  "recordsFiltered": {{ totalFiltered }}
}